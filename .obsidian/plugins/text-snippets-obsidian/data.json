{
  "snippets_file": "remark : >[!remark]\ncallout : >[!$end$]",
  "snippets": [
    "remark : >[!remark]",
    "callout : >[!$end$]"
  ],
  "endSymbol": "$end$",
  "newlineSymbol": "$nl$",
  "stopSymbol": "$tb$",
  "pasteSymbol": "$pst$",
  "useTab": true,
  "useSpace": false,
  "wordDelimiters": "$()[]{}<>,.!?;:'\"\\/",
  "isWYSIWYG": true
}